{
  "all": true,
  "include": ["src/**/*.js"],
  "exclude": [
    "node_modules/**",
    "tests/**",
    "coverage/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "reporter": ["lcov", "text", "html"],
  "report-dir": "./coverage",
  "temp-dir": "./.nyc_output",
  "check-coverage": false,
  "lines": 70,
  "statements": 70,
  "functions": 70,
  "branches": 70
}
